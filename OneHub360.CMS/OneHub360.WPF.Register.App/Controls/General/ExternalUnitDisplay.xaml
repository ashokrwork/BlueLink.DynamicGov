<UserControl x:Class="OneHub360.WPF.Register.App.Controls.General.ExternalUnitDisplay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OneHub360.WPF.Register.App.Controls.General"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             x:Name="ExternalUnitDisplayControl">
    <WrapPanel FlowDirection="RightToLeft">
        <Image Width="36" Name="imgOrganization"></Image>
        <StackPanel>
            <Label Name="lblTitle"></Label>
            <Label Name="lblOrgTitle"></Label>
        </StackPanel>
    </WrapPanel>
</UserControl>
