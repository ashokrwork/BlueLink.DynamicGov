    </property>
    <property name="Entityid">
      <column name="EntityId" sql-type="uniqueidentifier" not-null="true" />
    </property>
    <property name="Subject">
      <column name="Subject" sql-type="varchar" not-null="true" />
    </property>
    <property name="From">
