    </property>
    <property name="FkReplyto">
      <column name="FK_ReplyTo" sql-type="uniqueidentifier" not-null="true" />
    </property>
    <property name="FkDocument">
      <column name="FK_Document" sql-type="uniqueidentifier" not-null="true" />
