<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="OneHub.NET.TimeManagement" namespace="OneHub.NET.TimeManagement" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="FiscalYear" table="FiscalYear" schema="dbo">
    <id name="Id" type="Guid">
      <column name="Id" not-null="true" sql-type="uniqueidentifier" />
      <generator class="guid.comb" />
    </id>
    <property name="Name" type="String">
      <column name="Name" not-null="true" length="256" sql-type="nvarchar" />
    </property>
    <property name="StartDate" type="DateTime">
      <column name="StartDate" not-null="false" sql-type="date" />
    </property>
    <property name="EndDate" type="DateTime">
      <column name="EndDate" not-null="false" sql-type="date" />
    </property>
    <property name="IsDeleted" type="Boolean">
      <column name="IsDeleted" default="0" not-null="true" sql-type="bit" />
    </property>
    <property name="CreationDate" type="DateTime">
      <column name="CreationDate" not-null="true" sql-type="datetime2" />
    </property>
    <property name="LastUpdateDate" type="DateTime">
      <column name="LastUpdateDate" not-null="false" sql-type="datetime2" />
    </property>
    <property name="CreatedBy" type="String">
      <column name="CreatedBy" not-null="true" length="256" sql-type="nvarchar" />
    </property>
    <property name="LastUpdatedBy" type="String">
      <column name="LastUpdatedBy" not-null="false" length="256" sql-type="nvarchar" />
    </property>
    <bag name="PublicHolidays" generic="true">
      <key not-null="true">
        <column name="FK_FiscalYear" not-null="true" sql-type="uniqueidentifier" />
      </key>
      <one-to-many class="PublicHoliday" />
    </bag>
    <bag name="Projects" inverse="true" generic="true">
      <key>
        <column name="FK_FiscalYear" />
      </key>
      <one-to-many class="Project" />
    </bag>
  </class>
</hibernate-mapping>