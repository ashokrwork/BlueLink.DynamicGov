<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="OneHub360.NET.Admin.Model" namespace="OneHub360.NET.Admin.Model" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <union-subclass name="JobTitle" table="JobTitles" extends="OneHub360.NET.Core.Model.NHEntity">
    
    <property name="Title" type="String">
      <column name="Title" not-null="true" length="4000" sql-type="nvarchar" />
    </property>
    <property name="Responsibilities" type="String">
      <column name="Responsibilities" not-null="false" length="4000" sql-type="nvarchar" />
    </property>
    <property name="Rank" type="Int32">
      <column name="Rank" not-null="false" precision="10" scale="0" sql-type="int" />
    </property>
    <property name="Description" type="String">
      <column name="Description" not-null="false" length="4000" sql-type="nvarchar" />
    </property>
    
  </union-subclass>
</hibernate-mapping>