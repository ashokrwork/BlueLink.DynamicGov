<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="OneHub360.NET.Admin.Model" namespace="OneHub360.NET.Admin.Model" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="EntityType" table="EntityType" discriminator-value="EntityType">
    <id name="Id" type="Guid">
      <generator class="guid.comb" />
    </id>
    <discriminator>
      <column name="Discriminator" not-null="true" />
    </discriminator>
    <property name="Name" type="String">
      <column name="Name" not-null="true" length="4000" sql-type="nvarchar" />
    </property>
    <property name="CreationDate" type="DateTime">
      <column name="CreationDate" not-null="true" sql-type="datetime2" />
    </property>
    <property name="IsDeleted" type="Boolean">
      <column name="IsDeleted" not-null="false" sql-type="bit" />
    </property>
    <property name="LastModified" type="DateTime">
      <column name="LastModified" not-null="true" sql-type="datetime2" />
    </property>
    
    <property name="CreatedBy" type="String">
      <column name="CreatedBy" not-null="true" length="256" sql-type="nvarchar" />
    </property>
  </class>
</hibernate-mapping>